//BruteForce Approach
class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
        
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        cout.tie(NULL);
        
        vector<int> temp;
        
        for(int i=0;i<n;i++){
            temp.push_back(nums[i]);
            temp.push_back(nums[n+i]);
        }
        
        return temp;
    }
};
